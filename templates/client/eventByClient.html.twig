
{% if listeE is not empty %}
    <table border="2">
        <tr>
            <td>Id</td>
            <td>Date Debut</td>
            <td>Date Fin</td>
            <td>Lieu</td>
            <td>Nom</td>
            <td>Categorie</td>
        </tr>
        {% for e in listeE %}
            <tr>
                <td>{{ e.id }}</td>
                <td>{{ e.dateD | date }}</td>
                <td>{{ e.dateF | date }}</td>
                <td>{{ e.lieu }}</td>
                <td>{{ e.nom }}</td>
                <td>{{ e.categorie.nom }}</td>
            </tr>
        {% endfor %}
    </table>
{% endif %}